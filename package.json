{
  "name": "preact-template-default",
  "version": "0.0.0",
  "private": true,
  "description": "Improved preact default starter template from https://github.com/preactjs-templates/default",
  "keywords": [],
  "homepage": "https://github.com/spurreiter/preactjs-templates-default#readme",
  "bugs": {
    "url": "https://github.com/spurreiter/preactjs-templates-default/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spurreiter/preactjs-templates-default.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "preact build",
    "dev": "preact watch",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx ./src",
    "serve": "sirv build --port 8080 --cors --single",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".jsx"
    ],
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ],
    "transform": {},
    "transformIgnorePatterns": []
  },
  "dependencies": {
    "preact": "^10.17.1",
    "preact-render-to-string": "^6.2.1",
    "preact-router": "^4.1.2"
  },
  "devDependencies": {
    "@testing-library/preact": "^3.2.3",
    "eslint": "^8.50.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-preact": "^4.1.0",
    "preact-cli": "^3.5.0",
    "sirv-cli": "2.0.2"
  }
}
